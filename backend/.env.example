# Backend Environment Configuration
# Copy this file to .env and update values

# Database Configuration
# For SQLite (development):
DATABASE_URL=sqlite:///./swat_auth.db

# For PostgreSQL (production):
# DATABASE_URL=postgresql://user:password@localhost/swat_db

# JWT Configuration
SECRET_KEY=your-super-secret-key-change-in-production-use-random-32-chars
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=10080  # 7 days

# API Configuration
DEBUG=False
LOG_LEVEL=INFO
